<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "订单列表", admin_orders_path  %></li>
      <li><%= link_to "新增订单", new_admin_order_path %></li>
      <li class="active"><%= link_to "修改订单", edit_admin_order_path %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"> 修改订单 </h2>
    <div class="inner">
      <%= form_for @order, :url => admin_order_path(@order), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>

