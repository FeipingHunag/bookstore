<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "类别列表", admin_categories_path  %></li>
      <li><%= link_to "新增类别", new_admin_category_path %></li>
      <li class="active"><%= link_to "修改类别", edit_admin_category_path %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"> 修改类别 </h2>
    <div class="inner">
      <%= form_for @category, :url => admin_category_path(@category), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>

